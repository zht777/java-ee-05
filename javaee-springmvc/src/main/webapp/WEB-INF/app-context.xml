<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://www.springframework.org/schema/context
       https://www.springframework.org/schema/context/spring-context.xsd">

    <bean id="homework" class="model.Homework" />
    <bean id="student" class="model.Student" />
    <bean id="submit" class="model.StudentHomework" />

    <bean id="Homework_jdbc" class="jdbc.HomeworkJdbc" />
    <bean id="StudentHomework_jdbc" class="jdbc.StudentHomeworkJdbc" />
    <bean id="Student_jdbc" class="jdbc.StudentJdbc" />

</beans>